@model IEnumerable<WebApplication1.Models.Alienator>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Percentage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inscription.CNE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Person.Rut)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Percentage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inscription.CNE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Person.Rut)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { rut = item.Rut, atentionnumber = item.AtentionNumber }) |
            @Html.ActionLink("Details", "Details", new { rut = item.Rut, atentionnumber = item.AtentionNumber }) |
            @Html.ActionLink("Delete", "Delete", new { rut = item.Rut, atentionnumber = item.AtentionNumber })
        </td>
    </tr>
}

</table>
